main ()
 {
 console.out.println(addition (2, 5)); //output -> 7
 console.out.println(subtract (6, 2)); //output -> 4
 console.out.println(division (8, 4)); //output -> 2 
console.out.println(multiplication (3, 2)); //output -> 6
 }
 private Int addition (Int num, Int num2)
 {
 return num + num2; 
} private Int subtract (Int num, Int num2)
 {
 return num - num2; 
} 
private Int division (Int num, Int num2)
 {
 return num / num2; 
}
 private Int multiplication (Int num, Int num2)
{
 return num * num2;
 }

